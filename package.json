{
  "name": "farmanossa-expo",
  "main": "index.js",
  "version": "1.1.0",
  "description": "Farmanossa Delivery - Data-driven pharmacy delivery management system",
  "author": "Henry Matheus Froio <henry.froio@csp-medicamentos.com.br>",
  "company": "CSP COMERCIO DE MEDICAMENTOS LTDA",
  "license": "PROPRIETARY",
  "private": true,
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "bump-version": "node ./scripts/bump-version.js",
    "clean-cache": "node ./scripts/clean-cache.js",
    "build:ios": "npm run clean-cache && npm run bump-version && node switch-platform.js ios && node security-manager.js build && eas build --platform ios --profile production && node security-manager.js secure",
    "build:android": "npm run clean-cache && npm run bump-version && node switch-platform.js android && node security-manager.js build && eas build --platform android --profile production && node security-manager.js secure",
    "security:secure": "node security-manager.js secure",
    "security:build": "node security-manager.js build",
    "platform:ios": "node switch-platform.js ios",
    "platform:android": "node switch-platform.js android",
    "pre-commit": "npm run security:secure",
    "pre-build": "npm run security:build"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
